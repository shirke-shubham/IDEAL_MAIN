TYPE "ST_MASTER_HIERARCHY_APPR" AS TABLE(
    "HIERARCHY_ID" NVARCHAR(10),
    "ENTITY_CODE" NVARCHAR(10),
    "LEVEL" INTEGER,
    "ROLE_CODE" NVARCHAR(10),
    "TYPE" NVARCHAR(10),
    "ACCESS_EDIT" NVARCHAR(1),
    "ACCESS_APPROVE" NVARCHAR(1),
    "ACCESS_SENDBACK" NVARCHAR(1),
    "ACCESS_REJECT" NVARCHAR(1),
    PRIMARY KEY("HIERARCHY_ID","TYPE")
)